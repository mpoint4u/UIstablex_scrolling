<?xml version="1.0" encoding="UTF-8"?>
<Widget
	w="480"
	h="800"
		>
	<HBox 
		name="'box'"
		skin:Paint-color="0xAEF17C"
		widthPt="100"
		heightPt="100"
		childPadding="0"
		>
		
		<VBox
			name="'holder'"
			w = "200"
			heightPt="50"
			skin:Paint-color  = "0x8D9FF3"
			skin:Paint-border = "4"
			childPadding="25"
			paddingTop="5"
            paddingBottom="10"
			>
				
				<Button id="'myButton1'"
						name="'btn'"
						skin:Paint-border="2"  
						skin:Paint-color  = "0xDAAFD3"
						height="25"
						widthPt="90"					
						text="'press me to change TF text..'"
                        on-click=""
						/>
                        <!--on-click="trace('Oops! You clicked me again!');"-->
		</VBox>
	</HBox>


    <!-- Footer menu -->
    <HBox id="'bottomMenu'" defaults="'BottomMenu'" skin:Paint-color="0x3F3F3F" widthPt="100">
        <!-- back button -->
        <Button ico-src="'ui/android/img/ico/light/mainBack.png'"  on-click="" />
        <!-- home button -->
        <Button ico-src="'ui/android/img/ico/light/mainHome.png'" on-click="" />
        <!-- recent button -->
        <Button ico-src="'ui/android/img/ico/light/mainRecent.png'"  on-click="" />
    </HBox>

</Widget>